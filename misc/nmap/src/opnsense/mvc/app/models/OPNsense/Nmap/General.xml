<model>
  <mount>//OPNsense/nmap/general</mount>
  <description>Nmap configuration</description>
  <version>0.0.2</version>
  <items>
    <ip type="NetworkField">
      <Required>N</Required>
    </ip>
    <turn type="OptionField">
                <Required>N</Required>
                <multiple>Y</multiple>
                <default></default>
                <OptionValues>
                        <ospf>Open Shortest Path First (OSPF)</ospf>
                        <connected>Connected routes (directly attached subnet or host)</connected>
                        <kernel>Kernel routes (not installed via the zebra RIB)</kernel>
                        <rip>Routing Information Protocol (RIP)</rip>
                        <static>Statically configured routes</static>
                </OptionValues>
        </turn>
    <drl type="BooleanField">
      <Required>Y</Required>
      <default>0</default>
    </drl>
    </items>
</model>
