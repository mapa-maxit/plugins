<model>
  <mount>//OPNsense/nmap/general</mount>
  <description>Nmap configuration</description>
  <version>0.0.2</version>
  <items>
    <ip type="NetworkField">
      <Required>N</Required>
    </ip>
    <turn type="OptionField">
                <Required>N</Required>
                <multiple>Y</multiple>
                <default></default>
                <OptionValues>
                        <Pingscan>Option1(Pingscan)</Pingscan>
                        <SYNScan>Option2(SYNScan)</SYNScan>
                </OptionValues>
    </turn>
    <drl type="BooleanField">
      <Required>Y</Required>
      <default>0</default>
    </drl>
    </items>
</model>
